          },
          {
            "Op": "Assign",
            "Input": "NameNoDash.Clean",
            "Output": "Product.Name",
            "Assign": true,
            "Condition": null,
            "Parameters": null
          },
          {
            "Op": "Concat",
            "Input": "Ignored",
