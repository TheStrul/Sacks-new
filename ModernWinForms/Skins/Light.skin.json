{
  "theme": "GitHub",
  "inheritsFrom": "BaseLight",
  "description": "Clean light skin",
  "palette": {
    "primary": "#0969DA"
  },
  "controls": {
    "ModernButton": {
      "states": {
        "normal": {
          "backColor": "#FAFBFC",
          "foreColor": "#0D1117",
          "borderColor": "#E0E6ED"
        },
        "hover": {
          "backColor": "#F6F8FB",
          "foreColor": "#0D1117",
          "borderColor": "#D0D7E0"
        },
        "pressed": {
          "backColor": "#EBEEF2",
          "foreColor": "#0D1117",
          "borderColor": "#B8C1CC"
        },
        "disabled": {
          "backColor": "#F5F7FA",
          "foreColor": "#B0B8C3",
          "borderColor": "#E5EBF0"
        }
      }
    },
    "ModernGroupBox": {
      "borderWidth": 1,
      "cornerRadius": 4,
      "states": {
        "normal": {
          "foreColor": "#0D1117",
          "borderColor": "#E0E6ED"
        }
      }
    },
    "ModernTextBox": {
      "borderWidth": 1,
      "cornerRadius": 4,
      "padding": {
        "left": 8,
        "top": 8,
        "right": 8,
        "bottom": 8
      },
      "states": {
        "normal": {
          "backColor": "#FFFFFF",
          "foreColor": "#0D1117",
          "borderColor": "#E0E6ED"
        },
        "focused": {
          "borderColor": "#0969DA"
        }
      }
    }
  }
}
