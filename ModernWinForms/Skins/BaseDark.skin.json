        },
        "disabled": {
          "backColor": "#0D1117",
          "foreColor": "#6E7681",
          "borderColor": "#21262D"
        }
      }
    },
    "ModernGroupBox": {
      "states": {
        "normal": {
          "foreColor": "#E6EDF3",
          "borderColor": "#30363D"
        }
      }
    },
    "ModernTextBox": {
      "states": {
        "normal": {
          "backColor": "#0D1117",
          "foreColor": "#E6EDF3",
          "borderColor": "#30363D"
