{
  "inheritsFrom": "Base",
  "description": "Base dark skin with complete dark color palette and default control mappings",
  "palette": {
    "primary": "#58A6FF",
    "background": "#0D1117",
    "surface": "#161B22",
    "text": "#E6EDF3",
    "border": "#30363D",
    "success": "#3FB950",
    "warning": "#D29922",
    "danger": "#F85149",
    "error": "#F85149",
    "info": "#58A6FF"
  },
  "controls": {
    "ModernButton": {
      "states": {
        "normal": {
          "backColor": "#161B22",
          "foreColor": "#E6EDF3",
          "borderColor": "#30363D"
        },
        "hover": {
          "backColor": "#1F2937",
          "borderColor": "#58A6FF"
